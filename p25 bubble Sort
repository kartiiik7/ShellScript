def bubble_sort(arr):
  for n in range(len(arr)-1,0,-1):
    for i in range(n):
      if arr[i]>arr[i+1]:
        arr[i],arr{i+1]=arr[i+1],arr[i]
arr=[]
n=int(input("Enter the Limit of Array:"))
for i in range(n):
  element=int(input("Enter the Element of Array:"))
  arr.append(element)
print("Unsorted List:")
print(arr)
bubble_sort(arr)
print("Sorted List:")
print(arr)
